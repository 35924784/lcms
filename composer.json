{
    "name": "wanglelecc/laravel-cms",
    "description": "The LaraCMS management system.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "laravel/framework": "^5.6",
        "apollopy/flysystem-aliyun-oss": "^1.2",
        "dingo/api": "2.0.0-alpha1",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~4.0",
        "fukuball/jieba-php": "^0.33",
        "gregwar/captcha": "^1.1",
        "guzzlehttp/guzzle": "~6.3",
        "hieu-le/active": "~3.5",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.6",
        "laravel/scout": "^5.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "league/flysystem-azure-blob-storage": "^0.1.4",
        "liyu/dingo-serializer-switch": "^0.3.0",
        "mews/captcha": "~2.0",
        "mews/purifier": "~2.0",
        "orangehill/iseed": "^2.5",
        "overtrue/easy-sms": "^1.0",
        "overtrue/laravel-lang": "~3.0",
        "overtrue/laravel-wechat": "~4.0",
        "overtrue/pinyin": "~3.0",
        "overtrue/wechat": "~4.0",
        "predis/predis": "~1.1",
        "socialiteproviders/qq": "^3.0",
        "socialiteproviders/weibo": "^3.0",
        "socialiteproviders/weixin": "^4.0",
        "socialiteproviders/weixin-web": "^3.0",
        "spatie/laravel-permission": "~2.7",
        "teamtnt/laravel-scout-tntsearch-driver": "^3.2",
        "tymon/jwt-auth": "~1.0.0-rc.1",
        "vinkla/hashids": "^5.1",
        "zhuzhichao/ip-location-zh": "^2.4",
        "anchnet/aliyun-openapi-php-sdk": "dev-master"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "barryvdh/laravel-debugbar": "^3.1",
        "orangehill/iseed": "^2.5",
        "overtrue/laravel-query-logger": "^1.0",
        "wanglelecc/laracms-generator": "dev-master",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Models\\": "tests/models",
            "Tests\\Controllers\\": "tests/controllers"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate",
			"@php artisan vendor:publish --tag=config",
			"@php artisan vendor:publish --tag=public",
			"@php artisan vendor:publish --tag=laracms-view-errors",
			"@php artisan storage:link"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        
    }
}
